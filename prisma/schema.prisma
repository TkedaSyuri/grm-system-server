generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Floor2 {
  id         Int    @id @default(autoincrement())
  roomNumber String
  roomState  String
}

model Floor3 {
  id         Int    @id @default(autoincrement())
  roomNumber String
  roomState  String
}

